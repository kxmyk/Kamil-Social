{{ form_start(form) }}
<div>{{ form_errors(form) }}</div>

<div>
    {{ form_label(form.title, 'Please enter the post title') }}
</div>
<div>
    {{ form_widget(form.title, {attr: {'style' : 'outline: none'}}) }}
</div>
<div>
    {{ form_label(form.text, 'Please enter the post text') }}
</div>
<div>
    {{ form_widget((form.text)) }}
</div>


{{ form_end(form) }}